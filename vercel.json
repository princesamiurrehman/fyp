
{
	"version": 2, 
	"builds": [
		{
			"src": "easysnp/wsgi.py",
			"use": "@vercel/python",
			"config": {"maxLambdaSize": "15mb", "runtime": "python3.8"}
		},
		{
			"src": "build_files.sh",
			"use": "@vercel/static-build"
		}
	],
	"routes": [
		{
			"src": "/static/(.*)",
			"dest": "/static/$1"
		},
		{
			"src": "/(.*)",
			"dest": "easysnp/wsgi.py"
		}
		
	]
}
